using "Base";

var Int32 = (function() {
	return {
		MaxValue = 0x7fff_ffff,
		MinValue = -0x7fff_ffff,
		Parse = function(s) return Number.ParseInt32(s, Number.NumberStyles.Integer, Number.NumberFormatInfo),
		TryParse = function(s) return Number.TryParseInt32(s, Number.NumberStyles.Integer, Number.NumberFormatInfo)
	};
})();

var Byte = (function() {
	return {
		MaxValue = 0xff,
		MinValue = 0
	};
})();

var Environment = (function() {
	function GetTickCount() {
		return Microsoft.Win32.Win32Native.GetTickCount();
	}
	
	return {
		GetTickCount = GetTickCount
	};
})();

